  },
  "homepage": "https://github.com/neekey/react-webpack-spa#readme",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^1.8.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "less-plugin-rewrite-import": "^0.1.1",
